<?xml version="1.0" encoding="utf-8"?>
<ModuleConfigurations>
    <Module Class="VehicleComms" Type="Ardupilot">
    <Parameter Name="ProtocolParameters">
      <Parameter Name="Name">Mavlink</Parameter>
      <Parameter Name="Version">V2</Parameter>	  
    </Parameter>
	<Parameter Name="UDPParameters">
      <Parameter Name="ListenAddress">192.168.1.191</Parameter>
      <Parameter Name="ListenPortNumber">14550</Parameter>
    </Parameter>
	<Parameter Name="ModuleParameters">
      <Parameter Name="AirborneInstance">true</Parameter>
    </Parameter>
    </Module>
    <Module Class="ROS" Type="NASAPhase2">
    <Parameter Name="ModuleParameters">
      <Parameter Name="AirborneInstance">false</Parameter>
    </Parameter>
    <Parameter Name="LaserSensor">
      <Parameter Name="Name">laser</Parameter>
      <Parameter Name="Type">lidar_scan</Parameter>
    </Parameter>
    <Parameter Name="CameraSensor">
      <Parameter Name="Name">camera</Parameter>
      <Parameter Name="Type">rgb</Parameter>
    </Parameter>
  </Module>
</ModuleConfigurations>
