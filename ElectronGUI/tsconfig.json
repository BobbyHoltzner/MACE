{
  "compilerOptions": {
      "module": "CommonJS",
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "removeComments": false,
      "preserveConstEnums": true,
      "sourceMap": false,
      "jsx": "react",
      "target": "ES5",
      "experimentalDecorators": true,
      "outDir": "build/js",
      "inlineSourceMap": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "types": [
          "leaflet",
          "material-ui"
      ],
      "typeRoots": ["./node_modules/@types"]
    },
    "compileOnSave": false,
    "exclude": [
        "node_modules/**",
        "typings/*"
    ]
}
