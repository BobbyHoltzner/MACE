{
  "compilerOptions": {
      "module": "CommonJS",
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "removeComments": false,
      "preserveConstEnums": true,
      "sourceMap": false,
      "jsx": "react",
      "target": "ES5",
      "experimentalDecorators": true,
      "outDir": "build/js",
      "inlineSourceMap": true
    },
    "compileOnSave": false,
    "exclude": [
        "node_modules"
    ]
}
